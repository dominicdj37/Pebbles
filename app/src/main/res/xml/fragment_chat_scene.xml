<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>




    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>





    <Transition
        android:id="@+id/bringDownFabButton"
        motion:constraintSetEnd="@+id/fabButtonDown"
        motion:constraintSetStart="@id/fabButtonUp"
        motion:motionInterpolator="easeInOut"
        motion:duration="500">
        <KeyFrameSet>
            <KeyPosition
                motion:motionTarget="@+id/addChatFabButton"
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.805"
                motion:percentY="0.204" />
        </KeyFrameSet>
    </Transition>


    <Transition
        android:id="@+id/bringUpFabButton"
        motion:constraintSetEnd="@id/fabButtonUp"
        motion:constraintSetStart="@+id/fabButtonDown"
        motion:motionInterpolator="easeInOut"
        motion:duration="500">
        <KeyFrameSet>
            <KeyPosition
                motion:motionTarget="@+id/addChatFabButton"
                motion:framePosition="50"
                motion:keyPositionType="parentRelative"
                motion:percentX="0.799"
                motion:percentY="0.216" />
        </KeyFrameSet>
    </Transition>


    <ConstraintSet android:id="@+id/fabButtonDown">
        <Constraint
            android:id="@+id/addChatFabButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="100dp"
            android:rotation="0"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/fabButtonUp">
        <Constraint
            android:id="@+id/addChatFabButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:rotation="765"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp" />
    </ConstraintSet>



    <Transition
        android:id="@+id/expandPeopleListHorizontal"
        motion:constraintSetEnd="@+id/peopleListHorizontalExpanded"
        motion:constraintSetStart="@id/peopleListCollapsed"
        motion:motionInterpolator="easeInOut"
        motion:duration="500">
    </Transition>

    <Transition
        android:id="@+id/expandPeopleListWhole"
        motion:constraintSetEnd="@+id/peopleListFullExpanded"
        motion:constraintSetStart="@id/peopleListHorizontalExpanded"
        motion:motionInterpolator="easeInOut"
        motion:duration="500">
    </Transition>

    <ConstraintSet android:id="@+id/peopleListCollapsed">
        <Constraint
            android:id="@id/peoplesLayout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="100dp"
            android:visibility="gone"
            motion:layout_constraintBottom_toBottomOf="@id/addChatFabButton"
            motion:layout_constraintStart_toEndOf="@id/addChatFabButton"
            motion:layout_constraintTop_toTopOf="@id/addChatFabButton"
            motion:layout_constraintVertical_bias="0.0" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/peopleListHorizontalExpanded">
        <Constraint
            android:id="@id/peoplesLayout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@id/addChatFabButton"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/addChatFabButton"
            motion:layout_constraintTop_toTopOf="@id/addChatFabButton" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/peopleListFullExpanded">
        <Constraint
            android:id="@id/peoplesLayout"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="100dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@id/addChatFabButton"
            motion:layout_constraintTop_toTopOf="@id/addChatFabButton"
            motion:layout_constraintVertical_bias="0.0" />
    </ConstraintSet>




</MotionScene>